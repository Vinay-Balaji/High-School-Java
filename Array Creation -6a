 This code is supposed too take given data with the use of arrays and print them out
 ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
 // Lab06avst.java
 // This is the student starting file of Lab06a.
 //Created by Vinay Balaji 
 public class Lab06avst
 {
 public static void main (String[] args)
 {
 System.out.println("Lab06av100 by Vinay Balaji");
 int size = 10;
 School bhs = new School(size);//Constructor -Instnitaintg
 bhs.addData(); // Adding data
 System.out.println(bhs.toString()); //Using toString to print data
 }
 }
 class School
 {
 private Student[] students;
 private int size;
 public School (int s) //Constructor
 {
     size = s; 
 }
 public void addData()// Contains all of the data of the name age and gpa 
 {   
    String[] name = {"Tom","Ann","Bob","Jan","Joe","Sue","Jay","Meg","Art","Deb"};
    int[]age = {21,34,18,45,27,19,30,38,40,35}; 
    double[]gpa = {1.685,3.875,2.5,4.0,2.975,3.225,3.65,2.0,3.999,2.125};
    Student[] localstudents = new Student[size]; // Creating array for each student objects
    for (int i = 0; i < size; i++) {
        Student student = new Student(name[i].toString(),age[i],gpa[i]); //Creating a new student object
        localstudents[i] = student;// Taking the student object and assigning it to the local students array
    }
    students = localstudents; // Assigned to a private variable students so it can be accessed in toString class
 }
 public String toString()
 {
    String temp = "";
         for (int i = 0; i < size; i++) {
               temp = temp+students[i].toString();
          }
    return temp;
 }
 }
 
 
 ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
 //PLACE THIS FOLLOWING CODE INTO A SEPERATE FILE!!!!!
 // Student.java
 // Used by the Lab06 and Lab07 Assignments.

 public class Student
 {
 private String name;
 private int age;
 private double gpa;

 public Student (String n, int a, double g)
 {
 name = n;
 age = a;
 gpa = g;
 }

 public String getName() { return name; }
 public int getAge() { return age; }
 public double getGPA() { return gpa; }

 public String toString()
 {
 String temp = name + "" + age + " " + gpa + "\n";
 return temp;
 }
 }
